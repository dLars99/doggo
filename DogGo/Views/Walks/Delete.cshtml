@model DogGo.Models.ViewModels.WalksDeleteModel

@{
    ViewData["Title"] = "Delete";
}

<h1>Delete</h1>

<h3>Select Walks to Delete</h3>
<div>
    <hr />
    <form asp-action="Delete">
        <div class="form-group">
            @Html.ListBoxFor(x => x.SelectedWalks, Model.Walks)
        </div>

        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a href="@Url.Action("Index", "Walker")">Back to List</a>
    </form>
</div>
